{
  "functions": {
    "source": "functions",
    "runtime": "nodejs18",
    "timeout": "60s",
    "memory": "256MB"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/vps/**",
        "function": "vpsApi"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
